	<script>

(b=>{
	const d=b.ownerDocument, walk=d.createNodeIterator(d.createDocumentFragment().appendChild(b.cloneNode(true)),1),r=walk.root,
	f=(e)=>{
		e.stopPropagation();
		const n=e.target;
		if(n.classList.toggle('collapsed')!==true && e.shiftKey){
		 for(let i=0,m=n.querySelectorAll('.collapsed'),l=m.length;i<l;++i){m[i].classList.remove('collapsed')};
		}
	};
	let n;
	while((n=walk.nextNode())!==null){
		const s=n.tagName;
		n.insertBefore(d.createTextNode(s),n.firstChild);
		if(n.children.length){
			n.onclick=f;
			n.classList.add('father','collapsed')
		};
	};
	b.parentNode.replaceChild(r,b);
})(document.body);

	</script>
